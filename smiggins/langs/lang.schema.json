  "type": "object",
  "additionalProperties": false,
  "required": [
    "meta"
  ],
  "properties": {
    "meta": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "fallback": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "maintainers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "past_maintainers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "version": {
      "type": "string"
    },
    "generic": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "username": {
          "type": "string"
        },
        "success": {
          "type": "string"
        },
        "copied": {
          "type": "string"
        },
        "none": {
          "type": "string"
