            "return_to_voting": {
              "type": "string"
            },
            "view_results": {
              "type": "string"
            },
            "vote_count": {
              "type": "object",
              "additionalProperties": false,
              "patternProperties": {
                "\\*|\\d+": {
                  "type": "string"
                }
              }
            }
          }
        },
        "cw": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "length": {
              "type": "object",
              "additionalProperties": false,
              "patternProperties": {
                "\\*|\\d+": {
                  "type": "string"
                }
              }
            },
            "has_quote": {
              "type": "string"
            },
            "has_poll": {
              "type": "string"
            }
          }
        },
        "quote": {
          "type": "object",
