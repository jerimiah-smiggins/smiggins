            "has_poll": {
              "type": "string"
            },
            "cant_view": {
              "type": "string"
            }
          }
        },
        "tl_refresh": {
          "type": "string"
        },
        "show_new": {
          "type": "object",
          "additionalProperties": false,
          "patternProperties": {
            "\\*|\\d+": {
              "type": "string"
            }
          }
        },
        "copied": {
          "type": "string"
        },
        "embedded": {
          "type": "string"
        },
        "deleted": {
          "type": "string"
        },
        "cw_short": {
          "type": "string"
        },
        "poll_short": {
          "type": "string"
        },
        "button": {
          "type": "string"
        },
        "placeholder": {
          "type": "string"
